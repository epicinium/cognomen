{
    "name": "@epicinium/cognomen",
    "version": "1.4.2",
    "description": "A resolver in order to shorten deeply nested relative path expression.",
    "keywords": [
        "babel",
        "babel-plugin",
        "module",
        "resolver",
        "alias",
        "rewrite",
        "resolve",
        "rename",
        "mapping",
        "import",
        "eslint",
        "eslint-plugin-import",
        "eslint-import-resolver",
        "epicinium",
        "cognomen"
    ],
    "homepage": "https://github.com/epicinium/cognomen",
    "bugs": {
        "url": "https://github.com/epicinium/cognomen/issues"
    },
    "license": "MIT",
    "author": {
        "name": "Yongbin Min",
        "email": "contact@cichol.com"
    },
    "main": "releases/index.js",
    "directories": {
        "lib": "releases",
        "test": "tests"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/epicinium/cognomen.git"
    },
    "scripts": {
        "build": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run build:prod; else npm run build:dev; fi",
        "build:dev": "NODE_ENV=development npm run compile",
        "build:prod": "NODE_ENV=production npm run compile",
        "build:watch": "npm run compile -- --watch --verbose",
        "compile": "npx babel sources --out-dir releases --delete-dir-on-start",
        "postcompile": "if [ \"$NODE_ENV\" = \"production\" ]; then npm run minify:prod; else npm run minify:dev; fi",
        "minify:dev": "find releases -type f -exec sh -c \"npx terser --toplevel --compress --beautify --output {} -- {}\" \\;",
        "minify:prod": "find releases -type f -exec sh -c \"npx terser --toplevel --compress passes=2 --mangle --output {} -- {}\" \\;",
        "test": "npm run test:eslint && npm run test:mocha",
        "test:eslint": "npx eslint .",
        "test:mocha": "npx mocha tests --reporter list"
    },
    "dependencies": {
        "@babel/helper-plugin-utils": "^7.13.0",
        "@babel/runtime": "^7.13.9",
        "eslint-import-resolver-node": "^0.3.4",
        "glob": "^7.1.6",
        "js-yaml": "^4.0.0",
        "json5": "^2.2.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.13.0",
        "@babel/core": "7.13.8",
        "@babel/plugin-proposal-export-default-from": "^7.12.13",
        "@babel/plugin-proposal-export-namespace-from": "^7.12.13",
        "@babel/plugin-transform-runtime": "^7.13.9",
        "@babel/preset-env": "^7.13.9",
        "babel-eslint": "^10.1.0",
        "chai": "^4.3.3",
        "eslint": "7.21.0",
        "eslint-config-airbnb-base": "^14.2.1",
        "eslint-plugin-import": "2.22.1",
        "mocha": "^8.3.1",
        "terser": "^5.6.0"
    },
    "peerDependencies": {
        "@babel/core": "^7.13.8",
        "eslint": "^7.21.0",
        "eslint-plugin-import": "^2.22.1"
    },
    "engines": {
        "node": "^14.16.0",
        "npm": "^6.14.11"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    }
}
